    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "dev-server": "encore dev-server",
        "dev": "encore dev",
        "watch": "encore dev --watch",
        "build": "encore production --progress",
        "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore assets",
        "lint:fix": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix assets && npm run format:fix",
        "format": "prettier ./assets ./cypress --check --config .prettierrc.js",
        "format:fix": "prettier ./assets ./cypress --write --config .prettierrc.js",
        "cypress:open": "node ./node_modules/cypress/bin/cypress open",
        "test": "jest assets/js --config jest.config.js --coverage",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{js,ts,vue}": [
            "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix --cache assets",
            "prettier ./assets ./cypress --write --config .prettierrc.js",
            "jest assets/js --config jest.config.js"
        ],
        "*.{json}": [
            "prettier package.json --tab-width 4 --write"
        ]
    },
    "dependencies": {
        "@heroicons/vue": "^1.0.6",
        "@stripe/stripe-js": "^1.48.0",
        "@vueuse/components": "^8.7.5",
