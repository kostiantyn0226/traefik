        "pinia": "^2.0.16",
        "postcss-loader": "^6.2.1",
        "swiper": "^9.1.1",
        "tailwindcss": "^3.1.3",
        "ts-loader": "^9.4.2",
        "v-calendar": "^3.0.0-alpha.8",
        "vee-validate": "^4.8.6",
        "vue-axios": "^3.4.1",
        "vue-lodash": "^2.1.2",
        "vue-plaid-link2": "^0.2.2",
        "vue-select": "^4.0.0-beta.3",
        "vue-toast-notification": "^3.0.4",
        "vue3-apexcharts": "^1.4.1",
        "vuedraggable": "^4.1.0",
        "vuejs-paginate-next": "^1.0.2",
        "yup": "^1.1.1"
    }
